{
  "name": "time-tracker",
  "displayName": "Time tracker",
  "description": "Track your working time",
  "version": "0.0.1",
  "publisher": "Alexandru Cambose",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.togglePause",
        "title": "Toggle Pause"
      },
      {
        "command": "extension.toggleBreak",
        "title": "Toggle Break"
      },
      {
        "command": "extension.startWorkSession",
        "title": "Start work session"
      },
      {
        "command": "extension.stopWorkSession",
        "title": "Stop work session"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2"
  }
}
