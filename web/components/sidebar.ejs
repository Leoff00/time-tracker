<div class="column is-3">
  <aside class="menu">
    <p class="menu-label">
      Days
    </p>
    <ul class="menu-list">
      <% if(days.length){ days.forEach(function(day){ %>
      <li><a class="<%=day.active && 'is-active'%>" href="/<%=day.url%>">
          <%=day.label%>
      </li></a>
      <% }) } else {  %>
      <% include nodata %>
      <% } %>
    </ul>
</div>